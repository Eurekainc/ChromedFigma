{
  "manifest_version": 2,
  "name": "ChromedFigma",
  "description": "Figma extensions for Google Chrome",
  "version": "0.0.1",
  "permissions": [],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "declarativeContent",
    "tabs"
  ],
  "options_page": "index.html#/options"
}

